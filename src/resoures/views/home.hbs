
  {{!-- {{#each data}}
    <li>{{this}}</li>
  {{/each}} --}}
  <div class="mt-4 container">
  <div class="row">
    {{#each data}}
    <div class="shoes-item col-sm-6 col-lg-4">
        <div class="card" style="width: 18rem; margin-bottom:1.6rem">
            <a href="/shoes/{{this.slug}}">
            <img src="{{this.image}}" class="card-img-top" alt="...">
            </a>
            <div class="card-body">
                <a href="/shoes/{{this.slug}}" style="text-decoration: none;">
                <h5 class="card-title" style="text-overflow: ellipsis; white-space:nowrap;overflow: hidden;">{{this.name}}</h5>
                </a>
                <p class="card-text">{{this.description}}</p>
                <a href="#" class="btn btn-primary">Buy</a>
            </div>
        </div>
    </div>
    {{/each}}
  </div>

  </div>

